<body>
    <%- include('../partials/nav.ejs') %>

    <% if (messages.error) { %>
        <p style="color:red"><%- messages.error %></p>
        <% } %>
          
        <% if (messages.success) { %>
            <p style="color:green"><%- messages.success %></p>
    <% } %>

   
    <div class="login register">
        <form action="/register/new_tc_prof" method="post">
            <h1>Register</h1>
            <div class="profile">
                <img src="https://www.exaltedchristchurch.com/wp-content/uploads/2017/02/blank-profile-picture-png-transparent.png" alt="">
            </div>

            <div class="">
                <label for="S_Id">Company ID: </label>
                <br>
                <input type="number" name="c_Id" placeholder="1001">
            </div> 

            <div class="">
                <label for="S_Id">Company Name: </label>
                <br>
                <input type="text" name="c_nm" placeholder="Jade">
            </div> 
            <div class="">
                <label for="f_nm">Company Rate : </label>
                <br>
                <input type="text" name="c_rt" placeholder="Watson">
            </div> 

            <div class="submit">
                <input type="submit">
            </div>
        </form>
    </div>
</body>
</html>